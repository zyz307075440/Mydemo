 InitSeo();
 function InitSeo() {
    var seo = $("div[class='bottom_nav']");
    var detail = $("div[class='bottom_con']");
    detail.hide();
    seo.bind("click", function (evt) {
        var element = Event.element(evt);
        if (element.is("a") || element.is("li")) {
            if (element.is("a")) element = element.parents("li");
            seo.find("li").removeClass("action");
            element.addClass("action");
            var index = seo.find("li").index(element);
            detail.hide();
            detail.eq(index).show();
        }
    } .bind(this));
}
    
    //邮件订阅
    function clickEmailSubmit() {
     var email = $("#email").val();
     if (validator.valid(email, "notEmpty & email") == false) {
         alert("请输入订阅邮箱或邮箱格式不正确！");
         return false;
     }
     elongAjax.callBack("http://hotel.elong.com/isajax/EmailSubscribe/getorignalemail", {}, function (res) {
         var isUpdateEmail = false;
         if (res.isLogin && res.orginalEmail != email) {
             if (confirm("与注册email不一致，问是否需要更新注册email")) {
                 isUpdateEmail = true;
             }
         }
         var requestParams = { email: email, subscribeTypes: [1, 2, 3, 4, 6, 8], isUpdateEmail: isUpdateEmail };
         elongAjax.callBack("http://hotel.elong.com/isajax/EmailSubscribe/Subscribe", requestParams, function (result) {
             alert(result.msg);
         }, false, "GET", null, "jsonp");
     }, false, "GET", null, "jsonp");
    }
    function emailFocus()
    {
     if($("#email").val()=="请输入您的E-mail")
     {
      $("#email").val("");
     }
    }
    function emailBlur()
    {
     if($("#email").val()=="")
     {
      $("#email").val("请输入您的E-mail");
     }
    }
    $("#email").bind("focus",emailFocus);
    $("#email").bind("blur",emailBlur);
    $("#btnSubEmail").bind("click",clickEmailSubmit);
    })();
</script>
    
    
    
    
    
    
